{
  "name": "doujin-lp-builder",
  "productName": "Criclify",
  "version": "1.0.0",
  "description": "Criclify : 同人サークルのためのホームページビルダー",
  "main": "dist/main/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:electron\"",
    "dev:renderer": "vite",
    "dev:electron": "wait-on http-get://localhost:5173 && npm run dev:main",
    "dev:main": "tsc -p tsconfig.main.json && cross-env NODE_ENV=development electron .",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "package": "electron-builder"
  },
  "keywords": [
    "electron",
    "doujin",
    "landing-page",
    "template"
  ],
  "author": "Doujin LP Project",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/jszip": "^3.1.2",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.7",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.72.7",
    "@fluentui/react-icons": "^2.0.315",
    "@types/adm-zip": "^0.5.7",
    "adm-zip": "^0.5.16",
    "ajv": "^8.12.0",
    "handlebars": "^4.7.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.4.7",
    "jszip": "^3.10.1"
  }
}
